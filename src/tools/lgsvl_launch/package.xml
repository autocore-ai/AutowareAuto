<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>lgsvl_launch</name>
  <version>1.0.0</version>
  <description>LGSVL simulator launch package.</description>
  <maintainer email="cyberphysicalrobotics@gmail.com">Taehyung Kim</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>autoware_auto_cmake</buildtool_depend>

  <!-- Building process will not be performed.-->
  <!-- This package is used for only launching.-->

  <exec_depend>ament_index_python</exec_depend>
  <exec_depend>autoware_auto_avp_demo</exec_depend>

  <test_depend>ament_cmake_gtest</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
