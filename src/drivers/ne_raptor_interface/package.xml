<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ne_raptor_interface</name>
  <version>1.0.0</version>
  <description>Vehicle Interface for Raptor DBW controllers</description>
  <maintainer email="neweagleraptorgithub@neweagle.net">New Eagle</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>autoware_auto_cmake</buildtool_depend>

  <depend>autoware_auto_common</depend>
  <depend>autoware_auto_msgs</depend>
  <depend>motion_common</depend>
  <depend>raptor_dbw_msgs</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>std_msgs</depend>
  <depend>time_utils</depend>
  <depend>vehicle_interface</depend>

  <test_depend>ament_cmake_gtest</test_depend>
  <test_depend>ament_index_python</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  <test_depend>autoware_testing</test_depend>

  <exec_depend>ros2_socketcan</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
