<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>lanelet2_global_planner</name>
  <version>0.0.1</version>
  <description>lanelet2 global path planner</description>
  <maintainer email="punnu@parkopedia.com">Punnu Pahiratt</maintainer>
  <license>Apache License 2.0</license>
  <author>Punnu Phairatt</author>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>autoware_auto_cmake</buildtool_depend>

  <depend>lanelet2_core</depend>
  <depend>lanelet2_io</depend>
  <depend>lanelet2_projection</depend>
  <depend>lanelet2_traffic_rules</depend>
  <depend>lanelet2_routing</depend>
  <depend>autoware_auto_msgs</depend>
  <depend>motion_common</depend>
  <depend>autoware_auto_geometry</depend>

  <build_depend>autoware_auto_common</build_depend>
  <build_depend>eigen</build_depend>
  <build_export_depend>eigen</build_export_depend>

  <test_depend>ament_cmake_gtest</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
